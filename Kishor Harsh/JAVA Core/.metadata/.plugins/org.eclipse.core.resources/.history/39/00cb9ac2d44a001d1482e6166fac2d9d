package com.cybage.model;

public class Product {
	
	int id;
	String name;
	int price;
	
	
	public int getId() {
		return id;
	}
	public void setId(int id) {
		this.id = id;
	}
	public String getName() {
		return name1;
	}
	public void setName(String name) {
		this.name1 = name;
	}
	public int getPrice() {
		return price1;
	}
	public void setPrice(int price) {
		this.price1 = price;
	}
}
