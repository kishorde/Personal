package com.cybage.exception;

//Create an array of Employees & display nos. of Employee objects created.
class Employee
{
	int empId;
	String name;
	Employee(int empId,String name)
	{
		this.empId=empId;
		this.name=name;
	}
	
	static int obj=0;
	public Employee()
	{
		obj++;
	}
}

public class Assignment2 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		Employee emp[]=new Employee[5];
		emp[0] = new Employee(101,"Kishor");
		emp[1] = new Employee(101,"Kishor");
		emp[2] = new Employee(101,"Kishor");
		emp[3] = new Employee(101,"Kishor");
		
		System.out.println(Employee.obj);
		
		
		

	}

}
